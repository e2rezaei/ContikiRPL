 to a grid.&#xD;
Parameters:   N = sqrt(total number of sensors)&#xD;
	      distance 	&#xD;
*/&#xD;
//------------------------------------------------------ &#xD;
&#xD;
/*&#xD;
GENERATE_MSG(1000, 'move');&#xD;
 &#xD;
var distance = 10000 ;&#xD;
&#xD;
var N = 5; &#xD;
&#xD;
 while(true) {&#xD;
&#xD;
&#xD;
&#xD;
  YIELD();&#xD;
   &#xD;
	if(msg.equals("move")) &#xD;
	 {&#xD;
            var count = 1;&#xD;
			 for(var i = 0; i &lt; N; i++)&#xD;
			 {&#xD;
			     for ( var j = 0 ; j&lt; N ; j++)&#xD;
			     {&#xD;
			               var id = count;&#xD;
&#xD;
                           	       count = count + 1;&#xD;
                        &#xD;
			               &#xD;
			              log.log("hi  " + id +"  " + i + "   " + j); &#xD;
			          &#xD;
			               var node1 = sim.getMoteWithID(id);&#xD;
			&#xD;
			&#xD;
			                node1.getInterfaces().getPosition().setCoordinates(i*distance , j*distance , 0);     &#xD;
			         &#xD;
			      }&#xD;
			&#xD;
			}&#xD;
    }&#xD;
&#xD;
}*/&#xD;
&#xD;
&#xD;
//ReadMe: It is completely similar to loglistener output. :)&#xD;
&#xD;
&#xD;
TIMEOUT(3601000);&#xD;
&#xD;
GENERATE_MSG(3600000, 'finish');&#xD;
&#xD;
while(true) {&#xD;
